        <button class="btn btn-default btn-large "  type="button"><i class="fa fa-plus fa-lg"></i> Event</button>
        <button class="btn btn-default btn-large " type="button"><i class="fa fa-plus fa-lg"></i> | <i class="fa fa-edit fa-lg"></i>  Calendar</button>
        </div>
      </div>

    </div>

  <div class="row"
    <div class="col-md-12">
    <% @calendars.each do |calendar| %>
    <%= render partial: 'tab_content', locals: {calendar: calendar} %>
    <% end %>
    </div>
  </div>
</div>



</div>
</div>



